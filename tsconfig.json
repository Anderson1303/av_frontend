{
  "compilerOptions": {
    "lib": [
      "dom",
      "dom.iterable",
      "esnext"
    ],
    "allowJs": true,
    "skipLibCheck": true,
    "strict": false,
    "noEmit": true,
    "incremental": true,
    "esModuleInterop": true,
    "module": "esnext",
    "moduleResolution": "node",
    "resolveJsonModule": true,
    "isolatedModules": true,
    "jsx": "preserve"
  },
  "paths": {
    "@/public/*": [
      "public/*"
    ],
    "@/types/*": [
      "src/_types/*"
    ],
    "@/utils/*": [
      "src/_utils/*"
    ],
    "@/com/*": [
      "src/components/*"
    ],
    "@/styles/*": [
      "src/styles/*"
    ]
  },
  "include": [
    "next-env.d.ts",
    "**/*.ts",
    "**/*.tsx"
, "pages/teste/index.jsx", "pages/teste.tsxjsx"  ],
  "exclude": [
    "node_modules"
  ]
}
